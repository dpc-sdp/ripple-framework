{
  "name": "@dpc-sdp/ripple-tide-api",
  "description": "Ripple API endpoints for Tide Drupal backend",
  "version": "2.18.1",
  "license": "Apache-2.0",
  "repository": "https://github.com/dpc-sdp/ripple-framework",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./types.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils.js",
    "./helpers": "./dist/helpers.js",
    "./types": "./types.d.ts",
    "./errors": "./dist/errors/errors.js",
    "./components": "./dist/nuxt/components",
    "./services": "./dist/services/index.js"
  },
  "scripts": {
    "clean": "(rimraf dist* && rimraf tsconfig.tsbuildinfo) | true",
    "build": "pnpm clean && tsc -p tsconfig.json && cp ./src/schema/openapi.yml ./dist/schema",
    "watch": "tsc -p tsconfig.json -w"
  },
  "dependencies": {
    "@nuxt/kit": "3.13.2",
    "axios": "^1.3.4",
    "change-case": "^4.1.2",
    "cheerio": "^1.0.0",
    "h3": "^1.9.0",
    "js-yaml": "^4.1.0",
    "jsonapi-parse": "^2.0.1",
    "lodash-es": "^4.17.21",
    "nuxt-proxy": "^0.4.1",
    "pathe": "^1.1.0",
    "qs": "^6.11.1",
    "ts-json-schema-generator": "^1.2.0",
    "typescript-json-schema": "^0.55.0",
    "udp-transport-winston": "^1.2.11",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nuxt/schema": "3.13.2",
    "@types/cheerio": "^0.22.35",
    "axios-mock-adapter": "^1.21.3",
    "defu": "^6.1.2",
    "rimraf": "^4.4.1"
  },
  "files": [
    "dist",
    "src",
    "!**/*.spec.*",
    "!**/*.json",
    "types.d.ts",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ]
}
